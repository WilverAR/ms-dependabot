{
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "main"
  ],
  "prConcurrentLimit": 1,
  "prHourlyLimit": 0,
  "separateMajorMinor": false,
  "separateMinorPatch": false,
  "separateMultipleMajor": false,
  "groupName": "security-fixes",
  "groupSlug": "security-fixes",
  "dependencyDashboard": false,
  "packageRules": [
    {
      "matchVulnerabilityAlerts": false,
      "enabled": false
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
